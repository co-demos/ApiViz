{
  "name": "apiviz",
  "version": "1.0.0",
  "description": "",
  "main": "scripts/main.js",
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "watch": "npm-run-all --parallel watch:*",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:js": "NODE_ENV=production browserify ./scripts/main.js -o ../static/build/bundle.js",
    "build:css": "node-sass --omit-source-map-url styles/main.scss ../static/build/bundle.css",
    "watch:js": "NODE_ENV=production watchify ./scripts/main.js -o ../static/build/bundle.js -v -d",
    "watch:css": "node-sass styles/main.scss ../static/build/bundle.css --watch --source-map-embed"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "browserify": {
    "transform": [
      "@nymag/vueify",
      "babelify"
    ]
  },
  "author": "DINSIC - Etalab - EIG 2 - Co-demos",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@nymag/vueify": "9.4.5",
    "babelify": "^10.0.0",
    "browserify": "^14.5.0",
    "bulma": "^0.7.2",
    "bulma-checkradio": "^2.1.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.1",
    "watchify": "^3.11.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bulma-carousel": "^3.0.0",
    "bulma-extensions": "^4.1.0",
    "d3-dsv": "^1.0.7",
    "exports-loader": "^0.7.0",
    "prunecluster": "^2.1.0",
    "vee-validate": "^2.1.7",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vue2-leaflet": "^1.2.3",
    "vue2-leaflet-markercluster": "^2.1.0",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  }
}
